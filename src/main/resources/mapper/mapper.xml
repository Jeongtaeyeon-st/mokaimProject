<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper 
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
 
<mapper namespace="io.github.mokaim.mapper">
 
    <select id="selectTest" resultType="int">
        SELECT
            COUNT(*)
        FROM test
    </select>
    
    <insert id="insertTest" parameterType="io.github.mokaim.domain.TestDTO">
    	INSERT INTO test
    	VALUES(#{id},#{name})
    		
    </insert>
    
    <select id="selectListTest" resultType="io.github.mokaim.domain.TestDTO">
    	SELECT * FROM test
    </select>
   
</mapper>